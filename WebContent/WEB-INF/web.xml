<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>PHMS</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.phms.controllers.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ObservationServlet</servlet-name>
    <servlet-class>com.phms.controllers.ObservationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ObservationServlet</servlet-name>
    <url-pattern>/observation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewDetailsServlet</servlet-name>
    <servlet-class>com.phms.controllers.ViewDetailsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewDetailsServlet</servlet-name>
    <url-pattern>/viewDetails</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ViewPatientServlet</servlet-name>
    <servlet-class>com.phms.controllers.ViewPatientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewPatientServlet</servlet-name>
    <url-pattern>/viewPatient</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RecommendationServlet</servlet-name>
    <servlet-class>com.phms.controllers.RecommendationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RecommendationServlet</servlet-name>
    <url-pattern>/insertRecommendation</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>DeleteAlertServlet</servlet-name>
    <servlet-class>com.phms.controllers.DeleteAlertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DeleteAlertServlet</servlet-name>
    <url-pattern>/deleteAlert</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CreateUserServlet</servlet-name>
    <servlet-class>com.phms.controllers.CreateUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CreateUserServlet</servlet-name>
    <url-pattern>/addUser</url-pattern>
  </servlet-mapping>
</web-app>